#%RAML 1.0
title: Facets tests
uses:
  common:  ./common.raml
types:
  formattedDate:
    facets:
      format:
        type: string
        (common.FacetInstanceValidator):  !include facetValidator.js
  myDate:
    type: formattedDate
    format: yyyy
    example: 213
/qq:
  get:
    queryParameters:
      mm:

    responses:
      200:

        body:
          application/json:
            type: myDate
            example: 1211
