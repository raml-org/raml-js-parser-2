#%RAML 0.8
title: MultipleTraitsToResource_exp
version: 1
baseUri: https://example.com/{version}
traits:
   - trait0: !include traits/trait0.raml
   - trait1: !include traits/trait1.raml
/root-resource:
  is: [ trait0, trait1 ]
  get:
    description: Get object
    queryParameters:
      id:
        type: number
        description: object id
      name:
        type: string
        description: object name
      name1:
        type: string
        description: object name1
    responses:
      200:
      404:
        description: not found
      250:
      251:
        body:
          applicaiton/json:
      454:
        description: not found
  post:
    description: Create object
    queryParameters:
      name:
        type: string
        description: object name
      id1:
        type: number
        description: object id1
      name1:
        type: string
        description: object name1
    responses:
      200:
      201:
        body:
          applicaiton/json:
      404:
        description: not found
      250:
      454:
        description: not found
