{
  "ramlVersion": "RAML10",
  "type": "Api",
  "specification": {
    "title": "GitHub API",
    "version": "v3",
    "baseUri": "https://api.github.com",
    "protocols": [
      "HTTPS"
    ],
    "resources": [
      {
        "description": "The currently authenticated User",
        "relativeUri": "/user",
        "displayName": "/user",
        "__METADATA__": {
          "primitiveValuesMeta": {
            "displayName": {
              "calculated": true
            }
          }
        },
        "relativeUriPathSegments": [
          "user"
        ],
        "absoluteUri": "https://api.github.com/user"
      },
      {
        "description": "All users",
        "relativeUri": "/users",
        "displayName": "/users",
        "resources": [
          {
            "description": "A specific user",
            "uriParameters": {
              "userId": {
                "name": "userId",
                "displayName": "userId",
                "typePropertyKind": "TYPE_EXPRESSION",
                "type": [
                  "integer"
                ],
                "required": true,
                "description": "The id of the user",
                "__METADATA__": {
                  "primitiveValuesMeta": {
                    "displayName": {
                      "calculated": true
                    },
                    "required": {
                      "insertedAsDefault": true
                    }
                  }
                }
              }
            },
            "relativeUri": "/{userId}",
            "displayName": "/{userId}",
            "__METADATA__": {
              "primitiveValuesMeta": {
                "displayName": {
                  "calculated": true
                }
              }
            },
            "relativeUriPathSegments": [
              "{userId}"
            ],
            "absoluteUri": "https://api.github.com/users/{userId}"
          }
        ],
        "__METADATA__": {
          "primitiveValuesMeta": {
            "displayName": {
              "calculated": true
            }
          }
        },
        "relativeUriPathSegments": [
          "users"
        ],
        "absoluteUri": "https://api.github.com/users"
      }
    ],
    "__METADATA__": {
      "primitiveValuesMeta": {
        "protocols": {
          "calculated": true
        }
      }
    }
  },
  "errors": []
}