#%RAML 1.0
title: Dropbox API
version: 1
baseUri: https://api.dropbox.com/{version}
securitySchemes:
     passthrough:
        description: |
            This API supports PassThroughSecurityScheme Authentication.
        type: Pass Through
        describedBy:
            queryParameters:
                query:
                    type: string
            headers:
                api_key:
                    type: string